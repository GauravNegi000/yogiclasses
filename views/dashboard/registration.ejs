<% include ../partials/dashboardhead %>
<% var role = currentUser.role %>

        <div id="page-wrapper">
            <div id="page-inner">
                <div class="row">
                    <% if(success && success.length > 0) { %>      
                    <div class="alert alert-success" role="alert">
                        <%= success %>
                    </div>
                    <% } %>
                    <% if(error && error.length > 0) { %>      
                    <div class="alert alert-danger" role="alert">
                        <%= error %>
                    </div>
                    <% } %>
                    <div class="col-md-12">
                        <h1 class="page-head-line">Registration Applications</h1>
                    </div>                    
                </div>
                <div class="tabbable-panel">
                    <div class="tabbable-line">
                        <ul class="nav nav-tabs ">
                            <li class="active">
                                <a href="#tab_default_1" data-toggle="tab" aria-controls="tab_default_1" aria-selected="true">
                                                    Student </a>
                            </li>
                            <li>
                                <a href="#tab_default_2" data-toggle="tab" aria-controls="tab_default_2" aria-selected="false">
                                                    Teacher </a>
                            </li>
                        </ul>
                        <div class="tab-content">
                            <div class="tab-pane active" id="tab_default_1">
                                <h1 class="page-subhead-line">It contains all the Student Registration Submitted.</h1>
                                <div class="table-responsive">
                                    <table class="table table-striped table-bordered table-hover">
                                        <thead>
                                            <tr>
                                                <th width="50">S.No.</th>
                                                <th width="400">Name</th>
                                                <th width="400">Subject</th>
                                                <th width="80">Status</th>
                                                <th colspan="3">Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% var count = 1; %>
                                            <% registrations.reverse().forEach((registration) => { %>
                                                <tr>
                                                    <td><%= count++ %></td>
                                                    <td><%= registration.name %></td>
                                                    <td><%= registration.subjects %></td>
                                                    <td>
                                                            <% if(registration.isActive == 1) { %> 
                                                            <a class="btn btn-md btn-success">Created</a>
                                                            <% } %>
                                                    </td>                                                                                           
                                                            <td>
                                                                <a href="/dashboard/<%= role %>/registrations/s/details/<%= registration._id %>"><button type="button" class="btn btn-md btn-primary">View</button> </a>
                                                            </td>
                                                            <td>
                                                                <% if(registration.isActive == 0) { %>
                                                                <form action="/dashboard/<%= role %>/registrations/s/<%= registration._id %>" method="POST">
                                                                <button type="submit" class="btn btn-md btn-success">Create</button>
                                                                </form>
                                                                <% } %>
                                                            </td>                                                            
                                                            <td>
                                                                <form action="/dashboard/<%= role %>/registrations/s/<%= registration._id %>?_method=DELETE" method="POST" onsubmit="return confirm('Are You Sure You Want to Delete'); " >
                                                                <button type="submit" class="btn btn-md btn-danger">Delete</button>
                                                                </form>
                                                            </td>                                                                                                                                        
                                                </tr> 
                                            <% })  %>
                                        </tbody>
                                    </table>
                                </div>                               
                            </div>
                            <div class="tab-pane" id="tab_default_2">
                                <h1 class="page-subhead-line">It contains all the Teacher Registration Submitted.</h1>
                                <div class="table-responsive">
                                    <table class="table table-striped table-bordered table-hover">
                                        <thead>
                                            <tr>
                                                <th width="50">S.No.</th>
                                                <th width="400">Name</th>
                                                <th width="400">Subjects</th>
                                                <th width="80">Status</th>
                                                <th colspan="3">Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% var count = 1; %>
                                            <% tregistrations.reverse().forEach((tregistration) => { %>
                                                <tr>
                                                    <td><%= count++ %></td>
                                                    <td><%= tregistration.name %></td>
                                                    <td><%= tregistration.subjects %></td>
                                                    <td>
                                                            <% if(tregistration.isActive == 1) { %> 
                                                            <a class="btn btn-md btn-success">Created</a>
                                                            <% } %>
                                                    </td>                                                                                           
                                                            <td>
                                                                <a href="/dashboard/<%= role %>/registrations/t/details/<%= tregistration._id %>"><button type="button" class="btn btn-md btn-primary">View</button> </a>
                                                            </td>
                                                            <td>
                                                                <% if(tregistration.isActive == 0) { %>
                                                                <form action="/dashboard/<%= role %>/registrations/t/<%= tregistration._id %>" method="POST">
                                                                <button type="submit" class="btn btn-md btn-success">Create</button>
                                                                </form>
                                                                <% } %>
                                                            </td>                                                            
                                                            <td>
                                                                <form action="/dashboard/<%= role %>/registrations/t/<%= tregistration._id %>?_method=DELETE" method="POST" onsubmit="return confirm('Are You Sure You Want to Delete'); " >
                                                                <button type="submit" class="btn btn-md btn-danger">Delete</button>
                                                                </form>
                                                            </td>                                                                                                                                        
                                                </tr> 
                                            <% })  %>
                                        </tbody>
                                    </table>
                                </div>                               
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /. PAGE INNER  -->
        </div>
        <!-- /. PAGE WRAPPER  -->
    </div>
    <!-- /. WRAPPER  -->

<% include ../partials/dashboardfoot %>
